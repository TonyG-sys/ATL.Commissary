#This code reads an Excel file and prints out the content of each cell in every sheet.

from openpyxl import load_workbook

# Load the Excel workbook
wb = load_workbook(r"C:\Users\TGonzalezGomez\AABA SUMMER CHELSEA A la Carte Recipes 2025.xlsx", data_only=True)

for sheet in wb.worksheets:
    print(f"\n===== START OF SHEET: {sheet.title} =====")
    
    for row in sheet.iter_rows(values_only=False):
        for cell in row:
            val = cell.value
            if val is not None:  # Only print cells with content
                print(f"{cell.coordinate}: {val}")
    
    print(f"===== END OF SHEET: {sheet.title} =====")
 
